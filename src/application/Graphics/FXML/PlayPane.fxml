<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="MainBorderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="204.0" prefWidth="510.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Graphics.FXML.PlayPaneController">
   <top>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" BorderPane.alignment="TOP_LEFT" />
   </top>
   <center>
      <TilePane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox prefWidth="150.0" promptText="Select a song" />
         </children>
      </TilePane>
   </center>
   <right>
      <Button fx:id="playButton" mnemonicParsing="false" onAction="#playButtonClicked" text="Let's Play!" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
         <BorderPane.margin>
            <Insets right="15.0" />
         </BorderPane.margin>
      </Button>
   </right>
</BorderPane>
